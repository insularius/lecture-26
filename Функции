//proc 18

double CircleS(double R) {
	double S = 3.14 * pow(R, 2);
	return S;
}
int main() {
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);
	setlocale(LC_ALL, "Russian");
	double R;
	for (int i = 0; i < 3; i++) {
		cin >> R;
		cout << "Площадь круга: " << CircleS(R) << endl;
	}
	
}

